{
  "name": "svc-export",
  "displayName": "svg-export",
  "description": "SVG Export Extension for VSCode that uses canvg",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "svc-export.exportAsPNG",
      "title": "Export SVG as PNG"
    }],
    "menus": {
        "explorer/context": [
            {
              "command": "svc-export.exportAsPNG",
              "when": "resourceFilename =~ /^.*\\.(svg|SVG)$/",
              "group": "1_svgactions"
            }
        ],
        "commandPalette": [
            {
                "command": "svc-export.exportAsPNG",
                "when": "never"
            }
        ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  },
  "dependencies": {
    "canvas": "^2.1.0",
    "canvg": "^4.0.0",
    "node-fetch": "^2.6.0",
    "xmldom": "^0.6.0"
  }
}
